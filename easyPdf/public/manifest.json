{
    "manifest_version" : 3,
    "name" : "easyPDF",
    "description" : "Converts the selected text of the webpage into a pdf.",
    "version" : "1.0",

    "action" : {
        "default_popup" : "index.html"
    },
    "permissions" : [
        "scripting",
        "activeTab",
        "storage"
    ],
    "host_permissions" : [
        "http://*/*",
        "https://*/*"
    ]
   
}